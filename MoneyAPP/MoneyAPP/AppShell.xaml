<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MoneyAPP.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MoneyAPP.Pages"
    Shell.FlyoutBehavior="Disabled">
    
    <ShellContent ContentTemplate="{DataTemplate local:HomePage}"/>
    <ShellContent ContentTemplate="{DataTemplate local:RecordAddPage}" />
    <ShellContent ContentTemplate="{DataTemplate local:UploadPage}" />
    <ShellContent ContentTemplate="{DataTemplate local:SettingsPage}" />
    <ShellContent ContentTemplate="{DataTemplate local:OtherPage}" />
    <ShellContent ContentTemplate="{DataTemplate local:RecordRevisePage}" />
    <ShellContent ContentTemplate="{DataTemplate local:SetCategoryListPage}" />
    <ShellContent ContentTemplate="{DataTemplate local:SetAccountListPage}" />
</Shell>
